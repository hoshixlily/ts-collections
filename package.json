{
    "name": "@discordelia/ts-collections",
    "version": "2.0.0-alpha",
    "description": "A collection of wrappers for common data structures in TypeScript",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "umd:main": "dist/index.umd.js",
    "private": false,
    "scripts": {
        "build": "microbundle -i index.ts",
        "docs": "typedoc --out ./docs/ index.ts -m commonjs -t ES6",
        "prebuild": "rimraf dist",
        "postbuild": "rimraf .rts2_*",
        "start": "microbundle watch -i index.ts",
        "test": "nyc _mocha --require ts-node/register --recursive tests/**/*.test.*"
    },
    "keywords": [
        "typescript",
        "collections",
        "data structures"
    ],
    "author": "discordelia",
    "license": "MIT",
    "files": [
        "dist",
        "readme.md"
    ],
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.3",
        "braces": "^3.0.2",
        "chai": "^4.2.0",
        "microbundle": "^0.12.3",
        "mocha": "^8.1.2",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "ts-node": "^9.0.0",
        "typedoc": "^0.18.0",
        "typescript": "^4.0.2"
    }
}
