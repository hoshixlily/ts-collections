{
    "name": "@mirei/ts-collections",
    "version": "13.0.3",
    "description": "A collection of wrappers for common data structures in TypeScript",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": false,
    "scripts": {
        "build": "webpack",
        "docs": "npx typedoc",
        "prebuild": "rimraf dist",
        "test": "nyc _mocha --require ts-node/register --recursive tests/**/*.test.* --timeout 5000"
    },
    "keywords": [
        "typescript",
        "collections",
        "data structures",
        "list",
        "linked list",
        "dictionary",
        "lookup",
        "queue",
        "stack",
        "tree",
        "enumerable",
        "linq"
    ],
    "author": {
        "name": "Nanahoshi"
    },
    "license": "MIT",
    "files": [
        "dist",
        "readme.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hoshixlily/ts-collections"
    },
    "publishConfig": {
        "access": "public"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ],
        "branches": [
            "main"
        ]
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/chai": "^4.3.11",
        "@types/chai-as-promised": "^7.1.8",
        "@types/mocha": "^10.0.6",
        "braces": "^3.0.2",
        "chai": "^4.3.10",
        "chai-as-promised": "^7.1.1",
        "chokidar": "^3.5.3",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "rimraf": "^5.0.5",
        "rxjs": "~7.8.1",
        "semantic-release": "^22.0.12",
        "source-map-support": "^0.5.21",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typedoc": "^0.25.4",
        "typescript": "^5.3.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "optionalDependencies": {
        "fsevents": "*"
    }
}
